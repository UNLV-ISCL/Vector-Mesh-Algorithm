//define head file for use std::unordered_map

#ifndef CREATE_MAP_H
#define CREATE_MAP_H


#include <iostream>
#include <string>
#include <unordered_map>

	typedef std::unordered_map<std::string,int> global_map;

	stringmap merge (stringmap a,stringmap b) {
	  stringmap temp(a); temp.insert(b.begin(),b.end()); return temp;
	}

#endif
